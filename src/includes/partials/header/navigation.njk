
<button class="tob-mobile-menu-button" id="tob-mobile-menu">
  {% svg "menu", "Open Menu", "tob-mobile-menu-open-icon" %}
  {% svg "close", "Close Menu", "tob-mobile-menu-close-icon" %}
</button>

<nav>
  <ul>
    {% for page in navigation.top %}
      <li>
        <a href="{{page.url}}">{{page.text}}</a>
      </li>
    {% endfor %}
    <li>
      <button class="tob-theme-switcher" type="button" data-theme-switcher=""> 
        {% svg "moon", "Switch to dark theme", "tob-theme-dark-icon" %}
        {% svg "sun", "Switch to light theme", "tob-theme-light-icon" %}
      </button>
    </li>
  </ul>
</nav>
